{
  "name": "light-bright",
  "version": "1.0.0",
  "description": "A simple light-bright app.",
  "main": "src/app.js",
  "scripts": {
    "start": "webpack-dev-server --open --config ./webpack.dev.js",
    "build": "webpack --config ./webpack.prod.js",
    "build:dev": "webpack --config ./webpack.dev.js",
    "test": "jest --coverage",
    "test:no-coverage": "jest",
    "test:watch": "npm run test:no-coverage -- --watch",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "format": "prettier --write ./src/**/*.{js,jsx,css,scss,json,html}",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:scss": "stylelint ./src/**/*.scss",
    "lint:js": "eslint ./src/**/*.{js,jsx}",
    "lint:fix": "npm run lint:fix:js && npm run lint:fix:scss",
    "lint:fix:scss": "npm run lint:scss -- --fix",
    "lint:fix:js": "npm run lint:js -- --fix"
  },
  "author": "Spyros Argalias",
  "license": "ISC",
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "stylelint-prettier": "^1.0.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "cypress": "^3.2.0",
    "cypress-testing-library": "^3.0.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-array-func": "^3.1.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-no-secrets": "^0.1.2",
    "eslint-plugin-no-use-extend-native": "^0.4.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.11.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-xss": "^0.1.9",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.6",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "prettier": "1.17.0",
    "pretty-quick": "^1.10.0",
    "react-testing-library": "^7.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^10.0.1",
    "stylelint-config-idiomatic-order": "^7.0.0",
    "stylelint-config-prettier": "^5.1.0",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-declaration-use-variable": "^1.7.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  }
}
